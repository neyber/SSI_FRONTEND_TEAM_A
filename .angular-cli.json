{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "ssi-a-frontend"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "../node_modules/bootstrap/dist/css/bootstrap.css",
        "../node_modules/font-awesome/css/font-awesome.min.css",
        "../node_modules/slick-carousel/slick/slick.css",
        "../node_modules/slick-carousel/slick/slick-theme.css",
        "../node_modules/angular2-busy/build/style/busy.css",
        "../node_modules/ngx-toastr/toastr.css",
        "assets/app.css",
        "assets/css/icomoon/style.css",
        "assets/css/flaticon.css",
        "assets/css/animate.css",
        "assets/css/Plugins/fancybox/jquery.fancybox.css",
        "assets/css/Plugins/bootstrap-datepicker/bootstrap-datepicker.min.css",
        "assets/css/Plugins/select2/select2.css",
        "assets/css/Plugins/perfect-scrollbar/perfect-scrollbar.min.css",
        "assets/css/Plugins/footable-bootstrap/footable.bootstrap.min.css",
        "assets/css/Plugins/toaster/toaster.min.css",
        "assets/css/Plugins/ng-tags-input/ng-tags-input.min.css",
        "assets/css/Plugins/ng-tags-input/ng-tags-input.bootstrap.min.css",
        "assets/css/layout.css",
        "assets/css/kyp.css",
        "assets/css/accounts.css",
        "../node_modules/ion-rangeslider/css/ion.rangeSlider.css",
        "../node_modules/ion-rangeslider/css/ion.rangeSlider.skinHTML5.css",
        "assets/css/mind-map.css",
        "assets/css/treant/mind-map.css",
        "assets/css/treant/Treant.css",
        "sass/styles.scss"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/bootstrap/dist/js/bootstrap.js",
        "../node_modules/ion-rangeslider/js/ion.rangeSlider.min.js",
        "../node_modules/moment/moment.js",
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/slick-carousel/slick/slick.min.js",
        "assets/js/Plugins/perfect-scrollbar/perfect-scrollbar.jquery.min.js",
        "assets/js/Plugins/fancybox/jquery.fancybox.js",
        "assets/js/custom.js",
        "assets/footable.js",
        "assets/zingchart.min.js",
        "assets/js/treant/treant.js",
        "assets/js/treant/raphael.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "devsit": "environments/environment.dev.ts",
        "devlocal": "environments/environment.devlocal.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**/*"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**/*"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**/*"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {
    }
  }
}
